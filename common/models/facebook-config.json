      "type": "string"
    },
    "apiToken": {
      "type": "string"
    },
    "tenantId": {
      "type": "string"
    },
    "apiKey": {
      "type": "string"
    },
    "environment": {
      "type": "string"
    },
    "domainName": {
      "type": "string"
    },
